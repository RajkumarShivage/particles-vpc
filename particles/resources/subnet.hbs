{{#resource "m:core" "base"
  type="AWS::EC2::Subnet"
}}
"Properties": {
  "CidrBlock": {{helper "m:core" "propertyValue" cidrBlock}},
  "AvailabilityZone": {
    "Fn::If": [
      "{{definedAZCond}}",
      {{helper "m:core" "propertyValue" availabilityZone}},
      {"Ref": "AWS::NoValue"}
    ]
  },
  "VpcId": {{helper "m:core" "propertyValue" vpcId}}
}
{{/resource}}
